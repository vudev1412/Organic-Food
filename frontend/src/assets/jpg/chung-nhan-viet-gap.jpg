<script>
  
 function formatCurrency(a) {
    var b = parseFloat(a).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1.").toString();
    var len = b.length;
    b = b.substring(0, len - 3);
    return b;
}   
    
  $(document).ready(function(){
  
  var html = $(".nd").html();
  var patt = /<p.+\[Products:.+].+<\/p>/;
  var patt2 = /\[Products:.+]/;
  var test = "";
  
  console.log(patt.test(html));
  
  while(patt.test(html) == true){
    var stringIds = html.match(patt2).toString();
    var ids = stringIds.substring(10,stringIds.length-1).trim();
  
    console.log(ids);
  
    $.ajax({
      type: "POST",
      url: "/ajax/get_json.php?action=product&action_type=product-list&ids="+ids,
      success: function (data) {
          if(data.length > 10) {
            var data = JSON.parse(data);
            var products = data.list;   
  
            var htmlProducts="<div class='clear'></div><div class='product-list'><ul class='ul'>";
            for(var i=0; i < products.length; i++){
                var price = "";
                var prodPrice = "";
                var specialOffer = "";
                var marketPrice = "";
                if(products[i].price > 0 ) price = formatCurrency(products[i].price)+"đ"
                else price = "Liên hệ";
               
                if(products[i].specialOffer != '')  specialOffer = "<div class='is_specialOffer'>Quà tặng</div>";
                else specialOffer = "";
                   
                if(products[i].marketPrice > products[i].price) marketPrice = formatCurrency(products[i].marketPrice) + "đ"
                else marketPrice = "" ; 
                
                htmlProducts+=`

                <li>
                  <a href="${products[i].productUrl}" class="p-img" target="_blank">
                    <img src="${products[i].productImage.large}" alt="${products[i].productName}" /> 
                    <span class="btn-xemngay">Xem ngay</span>
                  </a>
                  <a href="${products[i].productUrl}" class="p-name" target="_blank">${products[i].productName}</a>
                  <div class="line-pro"></div>
                  <a href="javascript:addToShoppingCartStop('pro','${products[i].productId}',1,'${price}'); showPopup();" class="btn btn-blue">Đăng ký</a>
                  <a href="javascript:void(0)" class="btn btn-white call">Gọi để tư vấn<span>0937 619 299</span></a>
                </li>
                `;
          }
            htmlProducts+="</ul><div class='clear'></div></div>";
         
          var res = html.replace(patt, htmlProducts);
  
          $(".nd").html(res);
          
            html = res;
          
            }
      }, 
      async: false
    });
  }
  
  
   });
</script>
